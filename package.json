{
  "name": "saizeriya",
  "version": "0.3.0",
  "description": "サイゼリヤのメニューを取得する Node.js ライブラリ",
  "keywords": [
    "japan",
    "japanaese",
    "restaurant",
    "saizeriya"
  ],
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --emptyOutDir false",
    "preview": "vite preview",
    "prepare": "npx husky install",
    "test": "VITEST_MIN_THREADS=6 VITEST_MAX_THREADS=6 vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "format": "npx prettier . --write",
    "docs": "npx typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohidaka/saizeriya.git"
  },
  "author": "ryohidaka <39184410+ryohidaka@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryohidaka/saizeriya/issues"
  },
  "homepage": "https://github.com/ryohidaka/saizeriya#readme",
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.10.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "items-picker": "^0.1.0",
    "ts-case-convert": "^2.0.7"
  }
}
